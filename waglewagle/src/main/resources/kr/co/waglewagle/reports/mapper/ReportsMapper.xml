<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.waglewagle.reports.mapper.ReportsMapper">
	<insert id="insertReport" parameterType="kr.co.waglewagle.domain.ReportsVO">
		INSERT INTO reports (goods_id,users_id,reports_type,reports_content)
		VALUES(
			#{goods_id}
			,#{users_id}
			,#{reports_type}
			,#{reports_content}
		)
	</insert>
</mapper>